    <connection>DWprviprojekat</connection>
    <sql>SELECT orders.*,order_items.*,invoices.invoice_bigint,payments.payment_id,customer_payment_methods.customer_payment_id
FROM orders 
LEFT JOIN order_items
ON orders.order_id = order_items.order_id8
LEFT JOIN invoices
ON orders.order_id = invoices.order_id4
LEFT JOIN payments
ON invoices.invoice_bigint=payments.invoice_bigint3
LEFT JOIN customer_payment_methods
ON orders.customer_id3=customer_payment_methods.customer_id4



</sql>
